<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.2//EN" 
	"http://google-web-toolkit.googlecode.com/svn/tags/2.0.2/distro-source/core/src/gwt-module.dtd">
<module>
	<inherits name="com.google.gwt.user.User" />
	<!-- Require support for I18N -->
	<inherits name="com.google.gwt.i18n.I18N"/>
	<inherits name='com.google.gwt.junit.JUnit'/>
	<inherits name="gwtupload.GWTUpload" />
	
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="org.iplantc.core.JsonUtil"/>
	
	<entry-point class='org.iplantc.de.client.DiscoveryEnvironmentCommon' />
	
	<source path="client"/>
	<source path="shared"/>
</module>
